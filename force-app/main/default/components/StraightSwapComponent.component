        if(val.value=='Yes'){  
        alert('Please click save & continue to progress with CHB boiler lead and book a HSA visit.');
         if(e.style.display=="none"){
            e.style.display="block"
            f.style.display="none"
         }
         
        }else{
            if(f.style.display=="none"){
