                      alert('please Answer all the SS Conv. Questions');
                      return false;
                    }
               }
               
               if(matches == ""){
                 alert('Please close it');
               }
             
            } 
            return true;
         }
         else {
            if(!g)return true;
            if(!i)return true;
            
            if(i.style.display=="block"){
               var matches = [];
               var searchEles = document.getElementById(ajx4).getElementsByTagName("select");
               for(var i = 0; i < searchEles.length; i++) {
                    if(searchEles[i].value == ""){
                      matches.push(searchEles[i].value);
                      alert('please Answer all the SS Direct Questions');
                      return false;
                    }
               }
               
               if(matches == ""){
                 alert('Please close it');
               }
             
            } 
            return true;
         }
         
        
    
    }
    
    
