            <apex:commandbutton value="Save" action="{!saveUnavailDetails}" onclick="togglleDiv('');" oncomplete="togglleDiv('none');" rendered="{!recUnavail}" rerender="frm"/>                     
            <apex:commandbutton value="Validate" action="{!doNothing}" rendered="{!NOT(recAvail)}"/> 
        </apex:pageBlockButtons> 
        
        <!--Capture Employee details -->
